{
  "compilerOptions": {
    "lib": ["DOM", "es2019"],
    "target": "es5",
    "module": "commonjs",
    "moduleResolution": "node",
    "outDir": "./build",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "sourceMap": true,
    "esModuleInterop": true,
    "strict": true
  },
  "include": ["./*.ts", "./models/*.ts", "./static/*.ts"],
  "exclude": ["node_modules"]
}
